<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent">    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/desc_relative"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginStart="@dimen/fifty_dp"        android:layout_marginTop="@dimen/fifty_dp"        android:fontFamily="@font/chunk_five_print"        android:text="@string/description"        android:textColor="@color/light_blue"        android:textSize="@dimen/twenty_five_sp">    </androidx.appcompat.widget.AppCompatTextView>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/desc_text_relative"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/desc_relative"        android:layout_marginStart="@dimen/fifty_dp"        android:layout_marginTop="@dimen/twenty_dp"        android:layout_marginEnd="@dimen/twenty_dp"        android:text="@string/desc_text">    </androidx.appcompat.widget.AppCompatTextView>    <androidx.appcompat.widget.AppCompatImageView        android:id="@+id/phone_icon_relative"        android:layout_width="@dimen/thirty_dp"        android:layout_height="@dimen/thirty_dp"        android:layout_below="@+id/desc_text_relative"        android:layout_marginStart="@dimen/fifty_dp"        android:layout_marginTop="@dimen/thirty_dp"        android:src="@drawable/ic_baseline_local_phone_24">    </androidx.appcompat.widget.AppCompatImageView>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/telephone_number_relative"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/desc_text_relative"        android:layout_marginStart="@dimen/ninety_dp"        android:layout_marginTop="26dp"        android:text="@string/telephone_number"        android:textColor="@color/light_blue"        android:textSize="@dimen/twenty_five_sp">    </androidx.appcompat.widget.AppCompatTextView>    <androidx.appcompat.widget.AppCompatImageView        android:id="@+id/location_icon_relative"        android:layout_width="@dimen/thirty_dp"        android:layout_height="@dimen/thirty_dp"        android:layout_below="@+id/telephone_number_relative"        android:layout_marginStart="@dimen/fifty_dp"        android:layout_marginTop="@dimen/thirty_dp"        android:src="@drawable/ic_baseline_location_on_24">    </androidx.appcompat.widget.AppCompatImageView>    <androidx.appcompat.widget.AppCompatTextView        android:id="@+id/location_name_relative"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/telephone_number_relative"        android:layout_marginStart="@dimen/ninety_dp"        android:layout_marginTop="@dimen/thirty_dp"        android:fontFamily="@font/chunk_five_print"        android:text="@string/Luang_Prabang_Laos"        android:textColor="@color/light_blue"        android:textSize="@dimen/twenty_five_sp">    </androidx.appcompat.widget.AppCompatTextView>    <androidx.appcompat.widget.AppCompatImageView        android:id="@+id/google_map_relative"        android:layout_width="320dp"        android:layout_height="200dp"        android:layout_below="@+id/location_name_relative"        android:layout_marginStart="@dimen/fifty_dp"        android:layout_marginTop="@dimen/thirty_dp"        android:src="@drawable/luang_prabang">    </androidx.appcompat.widget.AppCompatImageView></RelativeLayout>